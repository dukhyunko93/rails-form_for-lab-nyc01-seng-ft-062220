<%= form_for @student do |f| %>
<h1>Edit Student Form</h1>
Student First Name:
<%= f.text_field :first_name, value: "#{@student.first_name}" %><br><br>
Student Last Name:
<%= f.text_field :last_name, value: "#{@student.last_name}" %><br><br>
<%= f.submit "Update Student" %><br><br>
<% end %>

<%= button_to "Cancel", students_path, method: "get" %>